{
  "type": "form",
  "actions": [],
  "wrapWithPanel": false,
  "name": "sample-edit-form",
  "api": {
    "method": "post",
    "url": "http://localhost:8888/api/form/save"
  },
  "initApi": "http://localhost:8888/api/form/get?valueId=${id}",
  "body": [
    {
      "type": "input-text",
      "name": "engine",
      "label": "Engine",
      "required": true,
      "zh-CN": {
        "label": "引擎"
      },
      "validations": {
        "isAlpha": true,
        "minLength": 3,
        "maxLength": 10
      }
    },
    {
      "type": "divider"
    },
    {
      "type": "input-text",
      "name": "browser",
      "label": "Browser"
    },
    {
      "type": "divider"
    },
    {
      "type": "input-text",
      "name": "platform",
      "label": "Platform(s)"
    },
    {
      "type": "divider"
    },
    {
      "label": "CSS grade",
      "type": "select",
      "name": "grade",
      "source": "http://localhost:8888/api/demo/options?formId=2"
    },
    {
      "type": "input-text",
      "name": "formId",
      "value": 2,
      "hidden": true
    },
    {
      "type": "input-text",
      "name": "pageX",
      "value": "page1",
      "hidden": true
    },
    {
      "type": "input-text",
      "name": "id",
      "hidden": true
    }
  ]
}